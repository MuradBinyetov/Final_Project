
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="Dashboard">
    <meta name="keyword" content="Dashboard, Bootstrap, Admin, Template, Theme, Responsive, Fluid, Retina">
    <title>Dashio - Bootstrap Admin Template</title>

    <!-- Favicons -->
    <link href="~/forAdmin/img/favicon.png" rel="icon">
    <link href="~/forAdmin/img/apple-touch-icon.png" rel="apple-touch-icon">

    <!-- Bootstrap core CSS -->
    <link href="~/forAdmin/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <!--external css-->
    <link href="~/forAdmin/lib/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="~/forAdmin/css/zabuto_calendar.css">
    <link rel="stylesheet" type="text/css" href="~/forAdmin/lib/gritter/css/jquery.gritter.css" />
    <!-- Custom styles for this template -->
    <link href="~/forAdmin/css/style.css" rel="stylesheet">
    <link href="~/forAdmin/css/style-responsive.css" rel="stylesheet">
    <script src="~/forAdmin/lib/chart-master/Chart.js"></script>
</head>

<body>
    <section id="container" class="col-3">

        <header class="header black-bg">
            <div class="sidebar-toggle-box">
                <div class="fa fa-bars tooltips" data-placement="right" data-original-title="Toggle Navigation"></div>
            </div>

            <a href="index.html" class="logo"><b>DASH<span>IO</span></b></a>

            <div class="nav notify-row" id="top_menu">

            </div>
            <div class="top-menu">
                <ul class="nav pull-right top-menu">
                    <li><a class="logout" href="login.html">Logout</a></li>
                </ul>
            </div>
        </header>
        <!--header end-->
        <!-- **********************************************************************************************************************************************************
        MAIN SIDEBAR MENU
        *********************************************************************************************************************************************************** -->
        <!--sidebar start-->

        <div class="row" style="margin-right:0;">
            <aside class="col-3">
                <div id="sidebar" class="nav-collapse ">
                    <!-- sidebar menu start-->
                    <ul class="sidebar-menu" id="nav-accordion">


                        <li class="mt">
                            <a class="active" asp-action="Index" asp-controller="Menus" asp-area="Admin">
                                <i class="fa fa-dashboard"></i>
                                <span>Menu</span>
                            </a>
                        </li>
                        <li class="mt">
                            <a class="active" asp-action="Index" asp-controller="SubMenus" asp-area="Admin">
                                <i class="fa fa-dashboard"></i>
                                <span>Submenu</span>
                            </a>
                        </li>
                        <li class="mt">
                            <a class="active" asp-action="Index" asp-controller="Categories" asp-area="Admin">
                                <i class="fa fa-dashboard"></i>
                                <span>Category</span>
                            </a>
                        </li>
                        <li class="mt">
                            <a class="active" asp-action="Index" asp-controller="Products" asp-area="Admin">
                                <i class="fa fa-dashboard"></i>
                                <span>Product</span>
                            </a>
                        </li>
                        <li class="mt">
                            <a class="active" asp-action="Index" asp-controller="Markas" asp-area="Admin">
                                <i class="fa fa-dashboard"></i>
                                <span>Marka</span>
                            </a>
                        </li>
                        <li class="mt">
                            <a class="active" asp-action="Index" asp-controller="Models" asp-area="Admin">
                                <i class="fa fa-dashboard"></i>
                                <span>Model</span>
                            </a>
                        </li>
                        <li class="mt">
                            <a class="active" asp-action="Index" asp-controller="Properties" asp-area="Admin">
                                <i class="fa fa-dashboard"></i>
                                <span>Properties</span>
                            </a>
                        </li>
                        <li class="mt">
                            <a class="active" asp-action="Index" asp-controller="Comments" asp-area="Admin">
                                <i class="fa fa-dashboard"></i>
                                <span>Comment</span>
                            </a>
                        </li>

                        <li class="mt">
                            <a class="active" asp-action="Index" asp-controller="Ratings" asp-area="Admin">
                                <i class="fa fa-dashboard"></i>
                                <span>Rating</span>
                            </a>
                        </li>
                        <li class="mt">
                            <a class="active" asp-action="Index" asp-controller="ProductCategories" asp-area="Admin">
                                <i class="fa fa-dashboard"></i>
                                <span>ProductCategories</span>
                            </a>
                        </li>
                        <li class="mt">
                            <a class="active" asp-action="Index" asp-controller="ProductProperties" asp-area="Admin">
                                <i class="fa fa-dashboard"></i>
                                <span>ProductProperties</span>
                            </a>
                        </li>

                    </ul>
                    <!-- sidebar menu end-->
                </div>
            </aside>
            <div class="col-9" style="margin-left:240px;margin-top:80px">
                @RenderBody()
            </div>
        </div>
       



    </section>
   

</body>

</html>